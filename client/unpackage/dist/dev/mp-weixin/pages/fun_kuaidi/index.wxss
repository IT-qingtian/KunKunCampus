.main {
  margin-top: 15px;
  width: 90%;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  position: absolute;
  box-sizing: border-box;
}
.main .type {
  font-size: 65 rpx;
  display: flex;
  justify-content: space-around;
}
.main .type text {
  transition: all 0.5s;
  color: #a9a6a7;
}
.main .type .active {
  padding-bottom: 5px;
  border-bottom: 5px solid #e9ea60;
  color: black;
  font-weight: bold;
}
.main .mode {
  transition: all 0.5s;
  position: relative;
}
.main .mode .type_1 {
  transition: all 0.5s;
}
.main .mode .type_2 {
  transition: all 0.5s;
  width: 100%;
  -webkit-transform: translateX(100%);
          transform: translateX(100%);
  position: absolute;
  top: 0;
}
.main .mode_1 {
  -webkit-transform: translateX(0);
          transform: translateX(0);
}
.main .mode_1 .type_2 {
  opacity: 0;
}
.main .mode_2 {
  -webkit-transform: translateX(-100%);
          transform: translateX(-100%);
}
.main .mode_2 .type_1 {
  opacity: 0;
}
.card {
  box-sizing: border-box;
  box-shadow: 0 3px 10px 1px rgba(0, 0, 0, 0.5);
  padding: 5px;
  margin-bottom: 7px;
  border-radius: 5px;
  background-color: #f7f7f7;
}
.card .card_item {
  border-top: 1px solid #726c6c;
  display: flex;
  padding: 5px;
  justify-content: space-between;
}
.card .card_item .left {
  font-weight: bold;
}
.card .card_item .right {
  font-size: 12px;
  color: #8f8f94;
}
.card .remarks {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width: 50%;
}
.card .deliver:before {
  content: "送";
  color: white;
  display: inline-block;
  text-align: center;
  width: 50 rpx;
  height: 50 rpx;
  line-height: 50 rpx;
  margin-right: 5px;
  background-color: lawngreen;
  border-radius: 50%;
}
.card .pay .left {
  flex: 7;
}
.card .pay .right {
  background-color: #fdc938;
  text-align: center;
  color: black;
  font-size: 15px;
  border-radius: 6px;
  flex: 3;
}
.card .card_item:first-child {
  border: none;
}
.bg .t {
  width: 750 rpx;
  height: 130px;
  border-bottom-left-radius: 10%;
  border-bottom-right-radius: 10%;
  background-color: #fdc938;
}

